<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="TextView">
        <attr name="font_path" format="string" />
        <attr name="textColor" format="color" />
        <attr name="hintColor" format="color" />
        <attr name="textType" format="enum">
            <enum name="XL" value="0" /> <!-- L DONE-->
            <enum name="L" value="1" /> <!-- M DONE-->
            <enum name="M" value="5" /> <!-- color_static -->
            <enum name="XS" value="3" /> <!-- XS DONE-->
            <enum name="S" value="2" /> <!-- S DONE-->
            <enum name="WigetText" value="4" /> <!-- White DONE-->
        </attr>
    </declare-styleable>

    <declare-styleable name="NSLayout">
        <attr name="layoutType" format="enum">
            <enum name="OPINION" value="0" /> <!-- OPINION -->
            <enum name="MULTIMEDIA" value="1" /> <!-- MULTIMEDIA -->
            <enum name="CARTOON" value="2" /> <!-- CARTOON -->
            <enum name="APPEXCLUSIVE" value="3" /> <!-- APPEXCLUSIVE -->
            <enum name="NORMAL_WIDGET" value="4" /> <!-- NORMAL_WIDGET -->
            <enum name="Black_Light" value="5" /> <!-- Black_Light -->
            <enum name="Black_Dark" value="6" /> <!-- Black_Dark -->
            <enum name="Black_Full" value="7" /> <!-- Black_Full -->
        </attr>
    </declare-styleable>

    <declare-styleable name="NSImageLogo">
        <attr name="logoType" format="enum">
            <enum name="Splash" value="0" />
            <enum name="Section" value="1" />
            <enum name="ToolbarTHP" value="2" />
        </attr>
    </declare-styleable>

    <declare-styleable name="NSImageButton">
        <attr name="btnType" format="enum">
            <enum name="arrow_no_line" value="0" />
            <enum name="back_circle" value="1" />
            <enum name="arrow_back" value="2" />
            <enum name="cross_circle" value="3" />
            <enum name="password_field" value="4" />
            <enum name="back_circle_dark_arrow" value="5" />
            <enum name="Section" value="6" />
        </attr>
    </declare-styleable>

    <declare-styleable name="NSIcon">
        <attr name="iconType" format="enum">
            <enum name="ttsPlay" value="0" />
            <enum name="share" value="1" />
            <enum name="favourite" value="2" />
            <enum name="bookmarked" value="3" />
            <enum name="unbookmark" value="4" />
            <enum name="fontSize" value="5" />
            <enum name="like" value="6" />
            <enum name="crown" value="7" />
            <enum name="overflow" value="8" />
            <enum name="search" value="9" />
            <enum name="dislike" value="10" />
            <enum name="unfavourite" value="11" />
            <enum name="ttsStop" value="12" />
            <enum name="comment" value="13" />
            <enum name="back" value="14" />
            <enum name="slider" value="15" />
            <enum name="cross" value="16" />
        </attr>
    </declare-styleable>

    <declare-styleable name="EmptyImageView">
        <attr name="emptyView" format="enum">
            <enum name="TH_Bookmark" value="0" />
            <enum name="THP_Bookmark" value="1" />
            <enum name="Briefing" value="2" />
            <enum name="Mystories" value="3" />
            <enum name="Suggestion" value="4" />
            <enum name="Crown" value="5" />
        </attr>
    </declare-styleable>

    <declare-styleable name="CustomTabLayout">
        <attr name="tabType" format="enum">
            <enum name="defaultTab" value="0" />
            <enum name="sectionTop" value="1" />
            <enum name="bottomTab" value="2" />
            <enum name="bookmarkTop" value="3" />
            <enum name="loginTop" value="4" />
        </attr>
    </declare-styleable>

    <declare-styleable name="TagFlowLayout">
        <attr name="auto_select_effect" format="boolean"></attr>
        <attr name="max_select" format="integer"></attr>
        <attr name="gravity">
            <enum name="left" value="-1" />
            <enum name="center" value="0" />
            <enum name="right" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="Theme">
        <attr name="discreteSeekBarStyle" format="reference"/>
    </declare-styleable>
    <declare-styleable name="DiscreteSeekBar">
        <attr name="dsb_min" format="integer|dimension"/>
        <attr name="dsb_max" format="integer|dimension"/>
        <attr name="dsb_value" format="integer|dimension"/>
        <attr name="dsb_mirrorForRtl" format="boolean"/>
        <attr name="dsb_allowTrackClickToDrag" format="boolean"/>
        <attr name="dsb_progressColor" format="color|reference"/>
        <attr name="dsb_trackColor" format="color|reference"/>
        <attr name="dsb_indicatorTextAppearance" format="reference"/>
        <attr name="dsb_indicatorColor" format="color|reference"/>
        <attr name="dsb_floaterColor" format="color|reference"/>
        <attr name="dsb_indicatorElevation" format="dimension"/>
        <attr name="dsb_indicatorFormatter" format="string|reference"/>
        <attr name="dsb_rippleColor" format="color|reference"/>
        <attr name="dsb_indicatorPopupEnabled" format="boolean"/>
        <attr name="dsb_trackHeight" format="integer|dimension"/>
        <attr name="dsb_scrubberHeight" format="integer|dimension"/>
        <attr name="dsb_thumbSize" format="integer|dimension"/>
        <attr name="dsb_indicatorSeparation" format="integer|dimension"/>
    </declare-styleable>


</resources>